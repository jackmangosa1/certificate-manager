{
  "name": "certificate-management",
  "version": "1.0.0",
  "description": "A comprehensive web application for managing certificates with React and TypeScript",
  "main": "index.js",
  "scripts": {
    "start": "webpack serve --config webpack/webpack.config.js --env env=dev",
    "build": "webpack --config webpack/webpack.config.js --env env=prod",
    "test": "jest",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,scss}\"",
    "type-check": "tsc --noEmit",
    "ci": "npm run lint && npm run type-check && npm test",
    "generate-api-client": "nswag openapi2tsclient /input:https://localhost:7203/swagger/v1/swagger.json /output:src/api/apiClient.ts /namespace:ApiClient"
  },
  "keywords": [
    "certificate",
    "management",
    "react",
    "typescript"
  ],
  "author": {
    "name": "Jack Mangosa",
    "email": "jackmangosa@gmail.com"
  },
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/jackmangosa1/certificate-manager"
  },
  "dependencies": {
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-router-dom": "^6.25.1",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@babel/core": "7.24.0",
    "@babel/plugin-transform-runtime": "7.24.0",
    "@babel/preset-env": "7.24.0",
    "@babel/preset-react": "7.23.3",
    "@babel/preset-typescript": "7.23.3",
    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": "^9.7.0",
    "@svgr/webpack": "^8.1.0",
    "@types/jest": "29.5.12",
    "@types/react": "18.2.64",
    "@types/react-dom": "18.2.21",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^7.16.1",
    "@typescript-eslint/parser": "^7.16.1",
    "babel-loader": "9.1.3",
    "clean-webpack-plugin": "4.0.0",
    "css-loader": "^7.1.2",
    "dotenv-webpack": "^8.1.0",
    "eslint": "^8.57.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.9.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.34.4",
    "eslint-plugin-react-hooks": "^4.6.2",
    "globals": "^15.8.0",
    "html-webpack-plugin": "5.6.0",
    "jest": "29.7.0",
    "prettier": "3.2.5",
    "source-map-loader": "5.0.0",
    "style-loader": "^4.0.0",
    "ts-loader": "9.5.1",
    "typescript": "5.4.2",
    "webpack": "5.90.3",
    "webpack-cli": "5.1.4",
    "webpack-dev-server": "5.0.2"
  }
}
